{
  "build": {
    "frontendDist": "../src"
  },
  "bundle": {
    "active": true,
    "targets": [
      "msi",
      "deb",
      "appimage"
    ],
    "resources": {},
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "livelog-ui",
  "version": "0.0.1",
  "identifier": "com.livelog",
  "plugins": {},
  "app": {
    "withGlobalTauri": true,
    "trayIcon": {
      "iconPath": "icons/32x32.png",
      "iconAsTemplate": true
    },
    "windows": [
      {
        "title": "livelog",
        "width": 300,
        "height": 600,
        "resizable": true
      }
    ],
    "security": {
      "csp": null
    }
  }
}